{
  "Rules": [
    {
      "Name": "DailyBriefingCrawler",
      "Description": "매일 새벽 4시에 크롤링 실행",
      "ScheduleExpression": "cron(0 4 * * ? *)",
      "State": "ENABLED",
      "Targets": [
        {
          "Id": "1",
          "Arn": "arn:aws:lambda:ap-northeast-2:ACCOUNT_ID:function:localbriefing-scheduled-crawler",
          "Input": "{\"trigger\": \"scheduled\"}"
        }
      ]
    }
  ]
}